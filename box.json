{
    "alias": "sw.phar",
    "chmod": "0755",
    "directories": ["src"],
    "files": [
        "LICENSE",
        "README.md",
        "config.yaml.dist",
        "vendor/autoload.php"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["tests"],
            "in": [
                "vendor/composer",
                "vendor/symfony/console",
                "vendor/symfony/dependency-injection",
                "vendor/symfony/process",
                "vendor/symfony/yaml",
                "src"
            ]
        }
    ],
    "git-version": "package_version",
    "main": "bin/sw",
    "output": "sw.phar",
    "stub": true
}
